{
  "name": "template",
  "version": "1.0.0",
  "type": "module",
  "description": "Plantilla base para los proyectos que quiera desarrollar.",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "lint": "eslint src/**/*.js --fix",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "npm run prepare",
    "test": "jest --verbose  --detectOpenHandles",
    "test:watch": "cross-env DB_DATABASE=testFechaStore npm run test -- --watch"
  },
  "author": "Juan Falco (https://portafolio-juan-falco.netlify.app/)",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "generate-password": "^1.7.0",
    "jsonwebtoken": "9.0.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "mssql": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.4.2",
    "cross-env": "^7.0.3",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.1",
    "jest": "^29.4.3",
    "lint-staged": "13.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "supertest": "^6.3.3"
  },
  "imports": {
    "#Errors/*": "./src/errors/*",
    "#Lib/*": "./src/lib/*",
    "#Config/*": "./src/config/*",
    "#Controllers/*": "./src/controllers/*",
    "#Routes/*": "./src/routes/*",
    "#Dto/*": "./src/dto/*",
    "#Database/*": "./src/database/*",
    "#Helpers/*": "./src/helpers/*"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,md}": "prettier --write"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learnthisacademy/nodejs-template.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/learnthisacademy/nodejs-template/issues"
  },
  "homepage": "https://github.com/learnthisacademy/nodejs-template#readme"
}
