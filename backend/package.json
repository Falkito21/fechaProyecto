{
  "name": "template",
  "version": "1.0.0",
  "type": "module",
  "description": "Plantilla base para los proyectos que quiera desarrollar.",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "lint": "eslint src/**/*.js --fix",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "npm run prepare"
  },
  "author": "Juan Falco (https://portafolio-juan-falco.netlify.app/)",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mssql": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1"
  },
  "imports": {
    "#Lib/*": "./src/lib/*",
    "#Config/*": "./src/config/*",
    "#Controllers/*": "./src/controllers/*",
    "#Routes/*": "./src/routes/*",
    "#Dto/*": "./src/dto/*",
    "#Database/*": [
      "./src/database/*"
    ],
    "#Helpers/*": [
      "./src/helpers/*"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,md}": "prettier --write"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learnthisacademy/nodejs-template.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/learnthisacademy/nodejs-template/issues"
  },
  "homepage": "https://github.com/learnthisacademy/nodejs-template#readme"
}
